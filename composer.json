{
    "name": "connor-lock05/laravel-admin",
    "description": "Adds an admin panel to a Laravel Application allowing for a UI to modify model data",
    "require": {
        "php": "8.2.*",
        "laravel/framework": "^11.0"
    },
    "suggest": {
        "spatie/laravel-permission": "Allows for use with Users and Roles"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ConnorLock05\\LaravelAdmin\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Connor-Lock05",
            "email": "monkeynutz1708@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "ConnorLock05\\LaravelAdmin\\LaravelAdminServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
